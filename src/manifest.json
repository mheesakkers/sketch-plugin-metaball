{
  "$schema": "https://raw.githubusercontent.com/BohemianCoding/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "icon": "icon.png",
  "name": "Metaball",
  "version" : "0.1",
  "description": "A simple plugin to calculate tangents between two circles.",
  "authorEmail" : "misha.heesakkers@gmail.com",
  "author" : "Misha Heesakkers",
  "disableCocoaScriptPreprocessor": false,
  "commands": [
    {
      "name": "Arc",
      "identifier": "bezier-tangents",
      "script": "./bezierTangents.js"
    },
    {
      "name": "Inner Tangents",
      "identifier": "inner-tangents",
      "script": "./innerTangents.js"
    },
    {
      "name": "Outer Tangents",
      "identifier": "outer-tangents",
      "script": "./outerTangents.js"
    }
  ],
  "menu": {
    "title": "Metaball",
    "items": [
      "bezier-tangents", "inner-tangents", "outer-tangents"
    ]
  }
}